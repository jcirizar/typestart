{
  "name": "typestart",
  "version": "1.0.0",
  "description": "Simple Express Typescript Server",
  "main": "dist/server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
